  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pageboard/server/issues"
  },
  "homepage": "https://github.com/pageboard/server#readme",
  "dependencies": {
    "postinstall-links": "^0.3.1",
    "upcache": "^0.6.5"
  },
  "links": {
    "upcache/nginx/conf.d/upcache-http.conf": "nginx/conf.d/",
    "upcache/nginx/snippets/upcache-location.conf": "nginx/snippets/",
    "upcache/nginx/snippets/upcache-server.conf": "nginx/snippets/"
  }
